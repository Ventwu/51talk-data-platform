{
  "name": "51talk-data-platform-backend",
  "version": "1.0.0",
  "description": "51Talk数据中台后端服务",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "echo \"Linting not configured yet\"",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "db:init": "ts-node src/scripts/initDatabase.ts",
    "db:test": "ts-node src/scripts/testDatabase.ts",
    "db:backup-schema": "ts-node -e \"import('./src/scripts/initDatabase').then(m => { const init = new m.DatabaseInitializer(m.getDatabaseConnector(), process.env.DB_TYPE || 'mysql'); init.backupSchema('./backup/schema.sql'); });\"",
    "db:drop-all": "ts-node -e \"import('./src/scripts/initDatabase').then(m => { const init = new m.DatabaseInitializer(m.getDatabaseConnector(), process.env.DB_TYPE || 'mysql'); init.dropAllTables(); });\""
  },
  "keywords": [
    "51talk",
    "data-platform",
    "dashboard",
    "analytics"
  ],
  "author": "51Talk Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.8",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "mysql2": "^3.14.4",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.13",
    "sequelize": "^6.37.3",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.2",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
